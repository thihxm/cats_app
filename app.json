{
  "expo": {
    "name": "CATS_app",
    "slug": "app_cats",
    "scheme": "app_cats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hvanin.app_cats"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "@config-plugins/react-native-webrtc"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9c43c0f7-0c06-4d7c-b972-dbd8b7bdb113"
      }
    }
  }
}
